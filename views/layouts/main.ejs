<!doctype html>
<html class="no-js" lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <title> Nodar SmartCash </title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- <link rel="apple-touch-icon" href="apple-touch-icon.png"> -->
  <!-- Place favicon.ico in the root directory -->
  <link rel="stylesheet" href="/css/modular-admin/vendor.css">
  <!-- Theme initialization -->
  <script>
    var themeSettings = (localStorage.getItem('themeSettings')) ? JSON.parse(localStorage.getItem('themeSettings')) :
      {};
    var themeName = themeSettings.themeName || '';
    if (themeName) {
      document.write('<link rel="stylesheet" id="theme-style" href="/css/modular-admin/app-' + themeName + '.css">');
    }
    else {
      document.write('<link rel="stylesheet" id="theme-style" href="/css/modular-admin/app.css">');
    }
  </script>
<!-- Global nodar config variable setup-->
<script>
  window.nodar = {};
</script>
</head>

<body>
  <div class="main-wrapper">
    <div class="app" id="app">
      <% include ../includes/header %>
      <% include ../includes/sidebar %>

      <%- body -%>

      <% include ../includes/footer %>

    </div>
  </div>
  <!-- Reference block for JS -->
  <div class="ref" id="ref">
    <div class="color-primary"></div>
    <div class="chart">
      <div class="color-primary"></div>
      <div class="color-secondary"></div>
    </div>
  </div>
  <!-- Modual Admin wraps up the jQuery and BS javascript for us along
    with javascript for the charts and some other custom stuff this template does
  -->
  <script src="/js/modular-admin/vendor.js"></script>
  <script src="/js/modular-admin/modular-admin.js"></script>

  <!-- Add feathers -->
  <script type="text/javascript" src="//unpkg.com/socket.io-client@1.7.3/dist/socket.io.js"></script>
  <script type="text/javascript" src="//unpkg.com/@feathersjs/client@^3.0.0/dist/feathers.js"></script>

  <!-- Add Vue -->
  <script src="https://cdn.jsdelivr.net/npm/vue@2.5.13/dist/vue.js"></script>

  <!-- Initialize the feathers api and authentication -->
  <script src="/js/feathers/api.js"></script>

  <!-- Add a place where each view's custom JS will go, notice it as after everything else -->
  <%- block('javascript') %>
</body>

</html>